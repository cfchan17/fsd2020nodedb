{{#each result}}
<table>
    <tr>
        <th>App Name</th>
        <td>{{this.name}}</td>
    </tr>
    <tr>
        <th>Category</th>
        <td>{{this.category}}</td>
    </tr>
    <tr>
        <th>Rating</th>
        <td>{{this.rating}}</td>
    </tr>
    <tr>
        <th>Downloads</th>
        <td>{{this.installs}}</td>
    </tr>
</table>
<br>
<hr>
{{/each}}